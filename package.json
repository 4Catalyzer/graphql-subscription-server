{
  "name": "@4c/graphql-subscription-server",
  "version": "1.1.0",
  "author": "4Catalyzer",
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/4Catalyzer/graphql-subscription-server.git"
  },
  "scripts": {
    "build": "4c build src",
    "format": "4c format --prettier-ignore .eslintignore .",
    "lint": "4c lint --prettier-ignore .eslintignore .",
    "prepublishOnly": "yarn run build",
    "release": "4c release",
    "tdd": "jest --watch",
    "test": "yarn lint && jest && yarn typecheck",
    "testonly": "jest",
    "typecheck": "tsc --noEmit && tsc -p test --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "4c lint --fix --prettier-ignore .eslintignore"
  },
  "prettier": {
    "printWidth": 79,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "testEnvironment": "node"
  },
  "release": {
    "pkgRoot": "lib",
    "extends": [
      "@4c/semantic-release-config"
    ]
  },
  "dependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.3",
    "@types/redis": "^2.8.16",
    "@types/socket.io": "^2.1.6",
    "express": "^4.17.1",
    "redis": "^3.0.0",
    "socket.io": "^2.3.0"
  },
  "peerDependencies": {
    "graphql": ">=0.12.3"
  },
  "devDependencies": {
    "@4c/babel-preset": "^8.0.1",
    "@4c/cli": "^2.1.11",
    "@4c/jest-preset": "^1.5.3",
    "@4c/prettier-config": "^1.1.0",
    "@4c/semantic-release-config": "^2.0.11",
    "@4c/tsconfig": "^0.3.1",
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-typescript": "^7.10.4",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-jest": "^26.3.0",
    "codecov": "^3.7.2",
    "eslint-config-4catalyzer": "^1.1.3",
    "eslint-config-4catalyzer-jest": "^2.0.8",
    "eslint-config-4catalyzer-typescript": "^3.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "graphql": "^15.3.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "redis-mock": "^0.52.0",
    "semantic-release": "^17.1.2",
    "travis-deploy-once": "^5.0.11",
    "typescript": "^4.0.3",
    "utility-types": "^3.10.0"
  },
  "engines": {
    "node": ">=v10.19.0"
  },
  "bugs": {
    "url": "https://github.com/4Catalyzer/graphql-subscription-server/issues"
  }
}
