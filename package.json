{
  "name": "@4c/graphql-subscription-server",
  "version": "0.0.1",
  "author": "4Catalyzer",
  "license": "MIT",
  "private": false,
  "main": "lib/index.js",
  "modules": "es/index/js",
  "scripts": {
    "build":
      "babel src -d lib --ignore __tests__ && BABEL_ENV=esm babel src --out-dir es --ignore __tests__",
    "check": "flow check",
    "lint":
      "npm run check && eslint . && prettier --list-different --ignore-path .eslintignore '**/*.{json,css,md}'",
    "format":
      "eslint . --fix && prettier --write --ignore-path .eslintignore '**/*.{json,css,md}'",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run test && npm run build",
    "test": "npm run lint && jest",
    "tdd": "jest --watch"
  },
  "prettier": {
    "printWidth": 79,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.{json,css,md}": [
      "prettier --write --ignore-path .eslintignore",
      "git add"
    ]
  },
  "jest": {
    "roots": ["<rootDir>/test"],
    "setupFiles": ["@babel/polyfill"],
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@4c/babel-preset-4catalyzer": "^1.2.1",
    "@babel/cli": "^7.0.0-beta.39",
    "@babel/core": "^7.0.0-beta.38",
    "@babel/plugin-proposal-async-generator-functions": "^7.0.0-beta.39",
    "@babel/polyfill": "^7.0.0-beta.40",
    "@babel/preset-flow": "^7.0.0-beta.39",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "eslint": "^4.18.1",
    "eslint-config-4catalyzer": "^0.3.3",
    "eslint-config-4catalyzer-flow": "^0.3.3",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.12.2",
    "eslint-plugin-prettier": "^2.5.0",
    "flow-bin": "^0.66.0",
    "graphql": "^0.13.1",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "lint-staged": "^7.0.0",
    "prettier": "^1.10.2",
    "redis-mock": "^0.21.0"
  },
  "peerDependencies": {
    "graphql": "^0.12.3"
  },
  "dependencies": {
    "express": "^4.16.2",
    "redis": "^2.8.0",
    "socket.io": "^2.0.4"
  }
}
